{
  "name": "gsaw-agent",
  "version": "1.0.0",
  "description": "Express API with JavaScript for LangChain agent",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "terminal": "node src/index.js --terminal",
    "build": "mkdir -p dist && cp -r src/* dist/",
    "serve": "node dist/index.js",
    "dev": "nodemon src/index.js",
    "create-tool": "node src/scripts/create-tool.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@langchain/community": "^0.3.36",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/openai": "^0.4.7",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "hyperliquid": "^1.7.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "secretvaults": "^0.0.0-alpha.1",
    "uuid": "^11.1.0",
    "viem": "^2.23.14"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0"
  }
}
